class Solution:
    def countBinarySubstrings(self, s: str) -> int:
        tmp = []
        cnt = 1
        for i in range(1, len(s)):
            if s[i] == s[i-1]:
                cnt+=1
            else:
                tmp.append(cnt)
                cnt = 1
        tmp.append(cnt)
        # print(tmp)
        return sum(min(tmp[i], tmp[i-1]) for i in range(1, len(tmp)))
