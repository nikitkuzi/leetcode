class Solution:
    def dominantIndices(self, nums: List[int]) -> int:
        pref = [0]*(len(nums)+1)
        pref[-1] = nums[-1]
        res = 0
        for i in range(len(nums)-2, -1, -1):
            pref[i+1] = nums[i] + pref[i+2]
            # print(pref[i+2], (len(nums)-i-1))
            if nums[i] > (pref[i+2]/(len(nums)-i-1)):
                res+=1
        # print(pref)
        return res

